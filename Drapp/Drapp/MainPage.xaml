<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:view="clr-namespace:Drapp.Metronome.View;assembly=Drapp"
             x:Class="Drapp.MainPage">
    <StackLayout>
        <StackLayout x:Name="stack" VerticalOptions="StartAndExpand">
            <ContentView/>
        </StackLayout>
        <StackLayout Orientation="Horizontal">
            <Button Text="Prev" Clicked="OnPreviousView" />
            <Button Text="Next" Clicked="OnNextView" />
        </StackLayout>
    </StackLayout>
</ContentPage>